CREATE TABLE IF NOT EXISTS `parvaz_det_tmp` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `parvaz_id` int(11) NOT NULL,
  `tarikh` date NOT NULL,
  `saat` time NOT NULL,
  `zarfiat` int(11) NOT NULL,
  `ghimat` int(11) NOT NULL,
  `typ` int(11) NOT NULL COMMENT 'صفریعنی عمومی یک یعنی خصوصی ۲ یعنی صندوق',
  `zakhire` int(11) NOT NULL,
  `j_id` int(11) NOT NULL,
  `poor_def` int(11) NOT NULL,
  `mablagh_kharid` int(11) NOT NULL DEFAULT '0',
  `saat_kh` time NOT NULL COMMENT 'ساعت ورود به مقصد',
  `can_esterdad` int(11) NOT NULL COMMENT 'یک یعنی قابل استرداد و صفر یعنی غیر قابل استرداد',
  `en` int(11) NOT NULL DEFAULT '1',
  `customer_id` int(11) NOT NULL DEFAULT '-1' COMMENT 'آژانس فروشنده',
  `tour_mablagh` int(11) NOT NULL DEFAULT '0',
  `toz` varchar(2000) COLLATE utf8_persian_ci NOT NULL,
  `sites_id` int(11) NOT NULL DEFAULT '-1',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM  DEFAULT CHARSET=utf8 COLLATE=utf8_persian_ci AUTO_INCREMENT=1 ;
